{
    "name": "ahmed.ibrahim/event-validator",
    "description": "Validate event object against a configured yaml file",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Ibrahim",
            "email": "ahmedalaa333@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EventValidator\\App\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventValidator\\Tests\\" : "tests/"
        }
    },
    "require": {
        "symfony/serializer": "^5.0",
        "symfony/property-access": "^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
